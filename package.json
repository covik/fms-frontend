    "@storybook/testing-library": "^0.0.13",
    "@storybook/testing-react": "^1.3.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.0.1",
    "babel-loader": "^8.3.0",
    "cypress": "^12.7.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "prettier": "2.8.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
