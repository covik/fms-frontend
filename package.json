{
  "name": "fms-react",
  "version": "0.0.1",
  "description": "Frontend for Fleet Management System",
  "author": "Mate Nakic",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "scripts": {
    "cy:dev": "cypress open --component --browser chrome",
    "cy:ct:chrome": "cypress run --component --browser chrome",
    "cy:ct:firefox": "cypress run --component browser firefox",
    "prepare": "husky install",
    "dev": "vite",
    "sb:dev": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "sb:build": "NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "type-check": "tsc -p tsconfig.json && tsc -p tsconfig.spec.json"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/firago": "^4.5.3",
    "@mui/material": "^5.11.5",
    "@react-google-maps/api": "^2.18.1",
    "@tanstack/react-query": "^4.29.3",
    "date-fns": "^2.29.3",
    "mdi-material-ui": "^7.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-custom-error": "^3.3.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@jest/globals": "^29.4.3",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
